{
  "name": "yalp2020-proto4",
  "version": "0.20.4",
  "description": "YALP 2020 prototype #4 (codemirror)",
  "x-main": "notebook.js",
  "main": "index.html",
  "scripts": {
    "scss": "node-sass --output-style compresses -o ./css ./css/scss",
    "watch:scss": "onchange ./css/scss -- npm run scss",
    "x-scss": "sass --watch ./css/scss:./css",
    "server": "live-server --port=2020 --host=127.0.0.1 --verbose --open=. --watch=. --watch=./js --watch=./css/*.css",
    "start": "run-p server watch:scss",
    "x-test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "djulien",
  "license": "ISC",
  "dependencies": {
    "codemirror": "^5.53.2",
    "live-server": "^1.2.1",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.1"
  },
  "old-dependencies": {
    "marked": "^0.8.2",
    "lodash": "^4.17.15",
    "splitpanes": "^2.2.1",
    "vue": "^2.6.11",
    "vue-multipane": "^0.9.5",
    "browser-sync": "?",
    "xyz": "^0.0.0"
  }
}
