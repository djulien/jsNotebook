{
  "name": "fxnotebook",
  "version": "0.20.6",
  "description": "crude FX notebook using markdown/OpenGL/JS+Node.js",
  "main": "server.js",
  "scripts": {
    "scss": "node-sass --output-style compresses -o ./NOTEBOOKs/css ./NOTEBOOKs/css/scss",
    "watch:scss": "onchange ./NOTEBOOKs/css/scss -- npm run scss",
    "x-scss": "sass --watch ./NOTEBOOKs/css/scss:./NOTEBOOKs/css",
    "scss-build": "sass ./NOTEBOOKs/css/scss:./NOTEBOOKs/css",
    "x-server": "live-server --port=2020 --host=127.0.0.1 --verbose --open=. --watch=. --watch=./js --watch=./NOTEBOOKs/css/*.css",
    "x-start": "run-p server watch:scss",
    "x-test": "run-s scss-build start-fxnb",
    "x-test-php": "./php-server.js myBOS",
    "x-start-fxnb": "./server.js ./NOTEBOOKs",
    "pretest": "npm run scss-build >/dev/null",
    "test": "./server.js ./NOTEBOOKs",
    "posttest": "echo 'done'"
  },
  "author": "djulien",
  "license": "TBD",
  "dependencies": {
    "colors": "^1.4.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "flatted": "^3.0.2",
    "magic-globals": "^0.9.1",
    "serve-index": "^1.9.1",
    "showdown": "^1.9.1",
    "xregexp": "^4.3.0"
  },
  "old-dependencies": {
    "colors": "git+https://github.com/djulien/colors.js-MODS.git",
    "codemirror": "^5.53.2",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "node-sass": "^4.13.1",
    "onchange": "^6.1.1",
    "marked": "^0.8.2",
    "lodash": "^4.17.15",
    "splitpanes": "^2.2.1",
    "vue": "^2.6.11",
    "vue-multipane": "^0.9.5",
    "browser-sync": "?",
    "xyz": "^0.0.0"
  },
  "devDependencies": {
    "sass": "^1.26.8"
  }
}
